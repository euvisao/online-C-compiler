#include <stdio.h>
#include <stdlib.h>

int main() {
    
   float a, b;
   
   printf("---------------------------\n");
   printf("digite um número qualquer: ");
   scanf("%f", &a);
   printf("---------------------------\n");
   printf("digite outro número qualquer: ");
   scanf("%f", &b);
   
   float divisao = a / b;
   
   printf("-----------------------------\n");
   printf("\na divisão dos números é: %.3f", divisao);
  
   return 0;
}
